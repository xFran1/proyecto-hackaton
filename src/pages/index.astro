<link rel="preconnect" href="https://fonts.googleapis.com">
<link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
<link href="https://fonts.googleapis.com/css2?family=Inter:ital,opsz,wght@0,14..32,100..900;1,14..32,100..900&family=Montserrat:wght@100..900&family=Poppins:ital,wght@0,100;0,200;0,300;0,400;0,500;0,600;0,700;0,800;0,900;1,100;1,200;1,300;1,400;1,500;1,600;1,700;1,800;1,900&display=swap" rel="stylesheet">

<style>
	body{
		font-family: "Montserrat", sans-serif;
		/* overflow-x: hidden; */
		background-color: #3a309686;
		opacity: 0;
		animation: 1.5s animacion-body ease-in;
		animation-fill-mode: forwards;
	}


	@keyframes animacion-body {
		from{

		}

		to{
			opacity: 1;
		}
	}
	
	.layour{
		overflow-x: hidden;
	}
</style>

<Layout class="layout">
